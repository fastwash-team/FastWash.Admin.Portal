{
  "name": "fastwashadminportal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "typegen:fast-wash-service": "openapi-typescript https://api.dev.fastwash.africa/swagger/v1/swagger.json --output ./src/types/api/fastwashservice.ts",
    "typegen:fast-wash-service-mac": "NODE_TLS_REJECT_UNAUTHORIZED=0 openapi-typescript https://api.dev.fastwash.africa/swagger/v1/swagger.json --output ./src/types/api/fastwashservice.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "localforage": "^1.10.0",
    "react-loading-skeleton": "^3.5.0",
    "react-multi-date-picker": "^4.5.2",
    "react-paginate": "^8.2.0",
    "react-to-print": "^3.0.4",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^5.1.1",
    "short-uuid": "^5.2.0",
    "written-number": "^0.11.1",
    "@tailwindcss/postcss": "^4.0.2",
    "@tailwindcss/vite": "^4.0.1",
    "@tanstack/react-query": "^5.66.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "flowbite-react": "^0.10.2",
    "formik": "^2.4.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "openapi-typescript": "^7.6.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.4",
    "sonner": "^1.7.3",
    "tailwindcss": "^4.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "yup": "^1.6.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tanstack/eslint-plugin-query": "^5.66.0",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "openapi-typescript": "^7.5.2"
  }
}
